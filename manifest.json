{
	"name": "Shook-eth Extension",
	"version": "1.0",
	"manifest_version": 2,
	"permissions": [ 
		"tabs",
		"contextMenus",
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["popup.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Shooketh Extension"
	}
}